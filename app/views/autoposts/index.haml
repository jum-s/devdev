.jump
- @autoposts.each do |auto|
  .columns.small-6.medium-4.large-3
    %hr
    = link_to auto do
      %h4 
        %small
          = auto.pocket_date.strftime("%e %b.")
          |
          = icon('clock-o')
          \~ #{get_reading_time(auto)}
      %h5
        %strong= auto.title
      %p.p-1
        = auto.excerpt